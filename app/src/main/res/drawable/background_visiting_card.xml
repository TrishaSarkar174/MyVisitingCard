<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">

          <solid android:color="#91AD64"/>
            <corners android:radius="20dp"/>
            <padding
                android:top="10dp"
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FDFBFB"/>
            <corners android:radius="10dp"/>
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="0dp"
                android:top="0dp"
                />


        </shape>
    </item>
</layer-list>